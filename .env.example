# Callnfy Dashboard Environment Variables
# Copy this file to .env and fill in your values

# API URL - Backend API endpoint
VITE_API_URL=https://api.callnfy.com/v1

# WebSocket URL for real-time notifications (must include /ws path)
VITE_NOTIFICATION_WS_URL=wss://notification.callnfy.com/ws

# For local development, use:
# VITE_API_URL=http://localhost:3000/v1
# VITE_NOTIFICATION_WS_URL=ws://localhost:3001/ws
